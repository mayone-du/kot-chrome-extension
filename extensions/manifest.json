{
  "manifest_version": 3,
  "version": "0.1",
  "name": "KOT Chrome Extention",
  "description": "KOT Chrome Extention",
  "action": {
    "default_popup": "./dist/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "scripting"]
}
